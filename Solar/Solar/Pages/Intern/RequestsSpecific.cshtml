        <div class="intern-tabs">
            <a asp-page="/Intern/Requests" class="intern-tab active-nav-tab">Anmodninger</a>
            <a asp-page="/Intern/Processed" class="intern-tab">Behandlet</a>
            <a asp-page="/Intern/Orders" class="intern-tab">Ordre</a>
        </div>
    </section>


    <section class="project-outer-container">
        <div class="project-main-container">
            <div class="full-size project-header">
                <div class="header-grid">
                    <p class="project-case-text">Sag:</p>
                    <p class="project-case-text">Firma:</p>
                    <p class="project-case-text">Manglende Fælter:</p>

                    <span></span>

                    <div>
                        <h2 class="project-case-text">Sag-nr 2993838</h2>
                        <h2 class="">Gadevej 2</h2>
                    </div>
                    <div>
                        <h2 class="">El-manden</h2>
                        <h2 class="">Thomas Hansen</h2>
                    </div>
                    <div>
                        <h2 class="missing-types">3</h2>
                    </div>
                    <a class="read-more-button">Godkend</a>
                </div>
            </div>

            <div class="full-size">
                <div class="content-grid">

                    <div class="side-container left-side">
                        <h2>Sags info</h2>
                        <div class="">

                            @*sags onfo tille denne sag*@
                            <div class="info-section">
                                <label>Sags Navn:</label>
                                <label>@Model.DataBaseInfo.CaseName</label>

                                <label>Adresse:</label>
                                <label>@Model.DataBaseInfo.Address</label>
                            </div>

                            @*tid og deadline*@
                            <div class="info-section">
                                <label>Start dato</label>
                                <label>@Model.DataBaseInfo.StartDate</label>

                                <label>Followup dato</label>
                                <label>@Model.DataBaseInfo.Followup</label>

                                <label>Deadline dato</label>
                                <label>@Model.DataBaseInfo.Deadline</label>
                            </div>

                            @*monterering og tagtyper*@
                            <div class="info-section">
                                <label>Monteage type:</label>
                                <label>@Model.RoofType.RoofType1</label>
                                <label>Tag type:</label>
                                <label></label>
                                <label>Ønskes øst/vest placering:</label>
                                <label>@Model.DataBaseInfo.Assembly.EastWestDirection</label>
                                <label>Hældning:</label>
                                <label>@Model.DataBaseInfo.Assembly.Slope</label>
                                <label>Højde på bygning:</label>
                                <label>@Model.DataBaseInfo.Assembly.BuildingHeight</label>
                            </div>

                            @*Battari*@
                            <div class="info-section">
                                <label>Forberedes til batteri:</label>
                                <label>@Model.DataBaseInfo.Battery.BatteryPrepare</label>
                                <label>Størrelse:</label>
                                <label>@Model.DataBaseInfo.BatteryRequest.Capacity</label>
                            </div>

                            @*Barttari klargøring*@
                            <div class="info-section">
                                <label>Forberedes til batteri:</label>
                                <label>@Model.DataBaseInfo.Battery.BatteryPrepare</label>
                                <label>Størrelse:</label>
                                <label>@Model.DataBaseInfo.BatteryRequest.Capacity <p> kWh</p></label>
                            </div>

                            @*Hordan skal solcellerne være*@
                            <div class="info-section">
                                <label>Solecelle ønske:</label>
                                @*dropdown menu udskrivning*@
                                @if (Model.DataBaseInfo.DimensioningId == 1)
                                {
                                    <label>Solcelleanlæg dimensioneres efter forbrug</label>
                                }
                                else if (Model.DataBaseInfo.DimensioningId == 2)
                                {
                                    <label>
                                        Solcelleanlæg dimensioneres efter størst
                                        mulig anlæg/stor produktion
                                    </label>
                                }
                                else if (Model.DataBaseInfo.DimensioningId == 3)
                                {
                                    <label>
                                        Solcelleanlæg dimensioneres efter ønsket
                                    </label>
                                    <label>@Model.DataBaseInfo.DimensioningConsumption</label>
                                }
                                @*burde ikke kunne ske*@
                                else
                                {
                                    <label>
                                        intet valgt
                                    </label>
                                }
                            </div>

                            @*Side 3.5 kommer kun vis man vælger 1 på side 3*@
                            @if (Model.DataBaseInfo.DimensioningId == 1)
                            {
                                @*dropdown menu udskrivning*@
                                <div class="info-section">
                                    <label>Anlægskategori:</label>
                                    @if (Model.DataBaseInfo.DimensioningConsumption.CategoryId == 1)
                                    {
                                        <label>Privat</label>
                                    }
                                    else if (Model.DataBaseInfo.DimensioningConsumption.CategoryId == 2)
                                    {
                                        <label>Erhverv</label>
                                    }
                                    else if (Model.DataBaseInfo.DimensioningConsumption.CategoryId == 3)
                                    {
                                        <label>Offentlig</label>
                                    }
                                    else
                                    {
                                        <label>ikke noteret</label>
                                    }

                                    <label>Nuværende forbrug i kWh:</label>
                                    <label>@Model.DataBaseInfo.DimensioningConsumption.CurrentConsumption</label>

                                    <label>Er/kommer der varmepumpe:</label>
                                    @if (Model.DataBaseInfo.DimensioningConsumption.HeatPump == true)
                                    @*boolien til teskt udskrevning*@
                                    {
                                        <label>Ja</label>
                                        <label>
                                            Er varmepumpe inkluderet i
                                            nuværende forbrug:
                                        </label>
                                        if (Model.DataBaseInfo.DimensioningConsumption.HeatPumpIncluded == true)
                                        {
                                            <label>Ja</label>
                                        }
                                        else
                                        {
                                            <label>Nej</label>
                                        }
                                    }
                                    else
                                    {
                                        <label>Nej</label>
                                    }

                                    <label>Størrelse hus</label>
                                    <label>@Model.DataBaseInfo.DimensioningConsumption.HouseSize<p> m2</p></label>

                                    <label>Er/kommer der elbil:</label>
                                    @*viser kun info vis der er en EV med i regnsabet*@
                                    @if (Model.DataBaseInfo.DimensioningConsumption.ElectricVehicle == true)
                                    {
                                        <label>Ja</label>

                                        <label>Er elbil inkluderet i nuværende forbrug:</label>
                                        @if (Model.DataBaseInfo.DimensioningConsumption.Evincluded == true)
                                        {
                                            <label>Ja</label>
                                        }
                                        else
                                        {
                                            <label>Nej</label>
                                        }
                                        <label>
                                            EHvor mange km køres pr. år:
                                        </label>
                                        <label>@Model.DataBaseInfo.DimensioningConsumption.EvKilometer</label>
                                    }
                                    else
                                    {
                                        <label>Nej</label>
                                    }
                                </div>
                            }

                            @*side 4 info flere bemærkninger fra indstallator*@
                            <div class="info-section">
                                <label>Bemærkninger:</label>
                                <p class="info-text">@Model.DataBaseInfo.Remarks</p>
                            </div>

                            @*kontakt info på indstalatør*@
                            <div class="info-section-contact">
                                <h2>Dine Kontakt oplysninger</h2>
                                <label>Navn: @Model.DataBaseInfo.Installer</label>
                                <label>Kontakt-Person: @Model.DataBaseInfo.Contact</label>
                                <label>Email: @Model.DataBaseInfo.Email</label>
                                <label>Tlf-Nummer: @Model.DataBaseInfo.PhoneNumber</label>
                            </div>
                        </div>
                    </div>
                    <div class="side-container right-side">
                        <div class="maps-container">
                            <iframe class="maps" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1598.3435076176263!2d12.076713834901978!3d55.631510429104985!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x46525fdb75e12093%3A0xba91a34039629adf!2sErhvervsforum%20Roskilde!5e0!3m2!1sda!2sdk!4v1685011731622!5m2!1sda!2sdk" style="border:0;" allowfullscreen="no" loading="lazy" referrerpolicy="no-referrer"></iframe>
                        </div>
                        <div class="full-size project-contact">
                            <h3>Indstalatør:</h3>
                            <label>Kontakt Person: @Model.DataBaseInfo.Contact</label>
                            <label>Indstalatør: @Model.DataBaseInfo.User.Installer</label>
                            <label>Nummer: @Model.DataBaseInfo.PhoneNumber</label>
                            <label>Mail: @Model.DataBaseInfo.Email</label>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        
    </section>


</main>